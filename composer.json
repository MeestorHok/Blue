{
    "name": "meestorhok/blue",
    "type": "library",
    "description": "Blue CMS is the premier SEO driven CMS for Laravel",
    "keywords": [ 
        "MeestorHok", "blue", "cms", "content", "management", "system", "laravel", "seo"
    ],
    "homepage": "https://github.com/MeestorHok/Blue",
    "license": "MIT",
    "authors": [
        {
            "name": "Jake Mitchell",
            "email": "jake@jmitchell.co",
            "homepage": "http://jmitchell.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "laravel/framework": "~5",
        "artesaos/seotools": "^0.9.2",
        "laravelcollective/html": "~5.2"
    },
    "autoload": {
        "psr-4": {
            "MeestorHok\\Blue\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan vendor:publish",
            "php artisan migrate"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan vendor:publish",
            "php artisan migrate"
        ]
    }
}
